<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#ffffff"/>
            <corners android:radius="9dp" />
            <padding
                android:right="10dp"
                android:left="10dp"
                android:bottom="5dp"
                android:top="5dp"/>
            <stroke
                android:width="1dp"
                android:color="#aeaeae"
                />
        </shape>
    </item>
</selector>